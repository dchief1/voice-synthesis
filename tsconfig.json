{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "rootDirs": ["src"],
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
